echo -e "IP address^Organization Name^Service Name"
for IP in $IPLIST
do
  RESULT=`whois $IP`
  ORG=`echo -n "$RESULT" | awk 'BEGIN { FS=": "; found=0 } /Org Name/ { if (found==0) { print $2; found=1} }'`
  SRV=`echo -n "$RESULT" | awk 'BEGIN { FS=": "; found=0 } /Service Name/ { if (found==0) { print $2; found=1} }'`
  # print result
  echo "$IP^$ORG^$SRV"
done
